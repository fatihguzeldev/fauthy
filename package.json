{
  "name": "fauthy",
  "version": "1.1.0",
  "description": "A CLI tool for managing TOTP MFA secrets",
  "icon": "logo.png",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "fauthy": "./dist/index.js"
  },
  "type": "module",
  "files": [
    "dist",
    "dist/types",
    "logo.png"
  ],
  "scripts": {
    "build": "tsc && tsc-alias --resolve-full-paths",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fatihguzeldev/fauthy.git"
  },
  "keywords": [
    "cli",
    "totp",
    "2fa",
    "authentication",
    "mfa",
    "security",
    "typescript"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Fatih GÃ¼zel <fatihguzeldev@gmail.com> (https://github.com/fatihguzeldev)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "open": "^10.1.0",
    "otplib": "^12.0.1"
  },
  "lint-staged": {
    "*.ts": [
      "npm run build",
      "eslint --fix",
      "prettier --write"
    ]
  }
}
